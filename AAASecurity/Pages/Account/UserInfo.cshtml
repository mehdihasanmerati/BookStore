@page
@model AAASecurity.Pages.Account.UserInfoModel
@{
}

<h3 class="mt-3">User Info</h3>

@if (User.Identity.IsAuthenticated)
{
    <table class="table table-bordered table-hover table-striped">
        <tr>
            <th>Name</th>
            <th>@User.Identity.Name</th>
        </tr>
        @foreach (var item in User.Claims)
        {
            <tr>
                <th>@item.Type</th>
                <th>@item.Value</th>
            </tr>
        }
    </table>
}
else
{
    <h2>Is not Athenticated</h2>
}